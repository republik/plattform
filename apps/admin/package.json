{
  "name": "@orbiting/admin-app",
  "private": true,
  "version": "1.1.0",
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch server --watch .env --watch server/constants.js server/server.js",
    "build": "next build",
    "start": "node server/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze": "ANALYZE=true next build",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l",
    "lint": "eslint --fix .",
    "zat:setup": "docker compose build",
    "zat:dev": "docker compose up zat-server",
    "zat:package": "docker compose run --rm zat-package"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ],
  "dependencies": {
    "@apollo/client": "~3.5.10",
    "@next/bundle-analyzer": "^12.3.1",
    "@project-r/styleguide": "*",
    "@republik/icons": "*",
    "@republik/nextjs-apollo-client": "*",
    "apollo-cache-inmemory": "^1.6.6",
    "d3-array": "^1.2.4",
    "d3-color": "^1.4.1",
    "d3-dsv": "^1.2.0",
    "d3-format": "^1.4.5",
    "d3-geo": "^1.12.1",
    "d3-scale": "^2.2.2",
    "d3-shape": "^1.3.7",
    "d3-time-format": "^2.3.0",
    "date-fns": "^2.22.1",
    "downshift": "^2.2.3",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "glamor": "^2.20.40",
    "graphql": "^15.3.0",
    "helmet": "^3.23.3",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "@republik/mdast-react-render": "*",
    "moment": "^2.29.1",
    "next": "^12.3.1",
    "next-transpile-modules": "^9.0.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-autosize-textarea": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-maskedinput": "^4.0.1",
    "react-textarea-autosize": "^7.1.2",
    "remark": "^10.0.1",
    "remark-react": "^5.0.1",
    "scroll-into-view": "^1.15.0",
    "subscriptions-transport-ws": "^0.9.19",
    "topojson": "^3.0.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.3.1",
    "@republik/eslint-config-frontend": "*",
    "@republik/tsconfig": "*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^16.11.21",
    "@types/react": "^18.0.28",
    "cross-env": "^5.2.1",
    "dotenv": "^6.2.0",
    "enzyme": "^3.11.0",
    "glob": "^7.1.6",
    "gsheets": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.6"
  }
}
