{
  "name": "@orbiting/admin-app",
  "private": true,
  "version": "1.1.0",
  "engines": {
    "node": "^14.4.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch server --watch .env --watch server/routes.js --watch server/constants.js server/server.js",
    "build": "next build",
    "start": "node server/server.js",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l",
    "lint": "eslint --fix .",
    "zat:setup": "docker compose build",
    "zat:dev": "docker compose up zat-server",
    "zat:package": "docker compose run --rm zat-package"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ],
  "dependencies": {
    "@project-r/styleguide": "*",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "d3-array": "^1.2.4",
    "d3-color": "^1.4.1",
    "d3-dsv": "^1.2.0",
    "d3-format": "^1.4.5",
    "d3-geo": "^1.12.1",
    "d3-scale": "^2.2.2",
    "d3-shape": "^1.3.7",
    "d3-time-format": "^2.3.0",
    "date-fns": "^2.22.1",
    "downshift": "^2.2.3",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "glamor": "^2.20.40",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "helmet": "^3.23.3",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.debounce": "^4.0.8",
    "mdast-react-render": "^1.2.0",
    "moment": "^2.29.1",
    "next": "^9.5.5",
    "next-routes": "^1.4.2",
    "next-transpile-modules": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-apollo": "^2.5.8",
    "react-autosize-textarea": "^5.0.1",
    "react-dom": "^17.0.2",
    "react-icons": "^3.11.0",
    "react-infinite-scroller": "^1.2.4",
    "react-maskedinput": "^4.0.1",
    "react-textarea-autosize": "^7.1.2",
    "remark": "^10.0.1",
    "remark-react": "^5.0.1",
    "scroll-into-view": "^1.15.0",
    "subscriptions-transport-ws": "^0.9.18",
    "topojson": "^3.0.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "dotenv": "^6.2.0",
    "enzyme": "^3.11.0",
    "@republik/eslint-config-frontend": "*",
    "glob": "^7.1.6",
    "gsheets": "^2.0.0",
    "nodemon": "^2.0.6"
  }
}
