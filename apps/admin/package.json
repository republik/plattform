{
  "name": "@orbiting/admin-app",
  "private": true,
  "version": "1.1.0",
  "engines": {
    "node": "^20.0.0"
  },
  "scripts": {
    "dev": "concurrently \"PORT=3006 next dev\" \"yarn gql:types --watch\"",
    "build": "yarn gql:types && next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze": "ANALYZE=true next build",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l",
    "lint": "eslint --fix .",
    "gql:types": "graphql-codegen --config graphql/configs/codegen-dev.ts",
    "zat:setup": "docker compose build",
    "zat:dev": "docker compose up zat-server",
    "zat:package": "docker compose run --rm zat-package"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@next/bundle-analyzer": "15.3.8",
    "@project-r/styleguide": "*",
    "@republik/icons": "*",
    "@republik/mdast-react-render": "*",
    "@republik/nextjs-apollo-client": "*",
    "@republik/theme": "*",
    "@sentry/nextjs": "^9.32.0",
    "apollo-cache-inmemory": "^1.6.6",
    "d3-array": "^1.2.4",
    "d3-color": "^1.4.1",
    "d3-dsv": "^1.2.0",
    "d3-format": "^1.4.5",
    "d3-geo": "^1.12.1",
    "d3-scale": "^2.2.2",
    "d3-shape": "^1.3.7",
    "d3-time-format": "^2.3.0",
    "date-fns": "^2.22.1",
    "downshift": "^2.2.3",
    "glamor": "^2.20.40",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.1",
    "next": "15.3.8",
    "prop-types": "^15.7.2",
    "react": "^19.2.3",
    "react-autosize-textarea": "^5.0.1",
    "react-dom": "^19.2.3",
    "react-infinite-scroller": "^1.2.4",
    "react-maskedinput": "^4.0.1",
    "react-textarea-autosize": "^7.1.2",
    "remark": "^10.0.1",
    "remark-react": "^5.0.1",
    "scroll-into-view": "^1.15.0",
    "subscriptions-transport-ws": "^0.11.0",
    "topojson": "^3.0.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.5",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/env": "15.3.8",
    "@republik/eslint-config-frontend": "*",
    "@republik/tsconfig": "*",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.1",
    "@types/node": "20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "concurrently": "^8.2.1",
    "glob": "^7.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^2.0.6"
  }
}
