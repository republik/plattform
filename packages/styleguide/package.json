  "scripts": {
    "commit": "git-cz",
    "prepublishOnly": "yarn prebuild && yarn build:lib",
    "build": "yarn build:lib && if [ \"$SERVER\" = \"styleguide\" ]; then yarn build:docs; fi",
    "build:lib": "tsc --project tsconfig.dist.json",
    "build:docs": "react-scripts build",
    "start": "serve -s build",
    "dev": "yarn dev:lib",
    "dev:lib": "tsc -w --project tsconfig.dist.json",
    "dev:docs": "concurrently \"yarn:dev:lib\" \"SKIP_PREFLIGHT_CHECK=true BROWSER=none PORT=3003 react-scripts start\"",
    "test": "CI=true react-scripts test",
    "test:dev": "react-scripts test",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./lib*\" | xargs wc -l",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release",
    "commitmsg": "validate-commit-msg",
    "translations-raw": "NODE_OPTIONS='-r dotenv/config' gsheets --key=1sMS5u4SIFY2loAmx8jftDXqTbq2RheJLY7FBeXSm1Tc --title=live --pretty --out src/lib/translations.json",
    "translations": "yarn translations-raw && git add -p src/lib/translations.json && git checkout -- src/lib/translations.json",
    "upgrade-interactive": "npx npm-check -u"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
