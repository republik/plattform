{
  "extends": "@orbiting/backend-modules-types/tsconfig-backend.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "declaration": true,
    "declarationDir": "build/@types",
    "declarationMap": true
  },
  "include": ["."],
