{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dotEnv": [
        ".env.production.local",
        ".env.development.local",
        ".env.local",
        ".env.production",
        ".env.development",
        ".env"
      ]
    },
    "@orbiting/backend-modules-auth#build": {
      "cache": false
    },
    "@orbiting/backend-modules-base#build": {
      "cache": false
    },
    "@orbiting/backend-modules-calendar#build": {
      "cache": false
    },
    "@orbiting/backend-modules-call-to-actions#build": {
      "cache": false
    },
    "@orbiting/backend-modules-databroom#build": {
      "cache": false
    },
    "@orbiting/backend-modules-dataloader#build": {
      "cache": false
    },
    "@orbiting/backend-modules-discussions#build": {
      "cache": false
    },
    "@orbiting/backend-modules-invoices#build": {
      "cache": false
    },
    "@orbiting/backend-modules-publikator#build": {
      "cache": false
    },
    "@orbiting/backend-modules-republik#build": {
      "cache": false
    },
    "@orbiting/backend-modules-republik-crowdfundings#build": {
      "cache": false
    },
    "@project-r/styleguide#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ],
      "env": [
        "SERVER"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^build",
        "build"
      ],
      "dotEnv": [
        ".env.test.local",
        ".env.local",
        ".env.test",
        ".env"
      ]
    },
    "prepare": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  },
  "globalDotEnv": [
    ".env.local",
    ".env"
  ]
}